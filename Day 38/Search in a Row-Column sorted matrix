class Solution {
    public static boolean matSearch(int mat[][], int x) {
        // your code here
        int n=mat.length;
        int m=mat[0].length;
        
        int i=0;
        int j=m-1;
        
        while(i<n && j>=0){
            int val=mat[i][j];
            if(val==x){
                return true;
            }else if(val>x){
                j--;
            }else{
                i++;
            }
        }
        
        return false;
    }
}
