class Solution {
    public ArrayList<Integer> intersect(int[] a, int[] b) {
        // code here
        HashSet<Integer> setA=new HashSet<>();
        for(int nums:a){
            setA.add(nums);
        }
        
        HashSet<Integer> resultset=new HashSet<>();
        for(int nums:b){
            if(setA.contains(nums)){
                resultset.add(nums);
            }
        }
        
        
        return new ArrayList<>(resultset);
    }
}
