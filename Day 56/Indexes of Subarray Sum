
class Solution {
    static ArrayList<Integer> subarraySum(int[] arr, int target) {
        // code here
        ArrayList<Integer> result=new ArrayList<>();
        int start=0;
        int currentsum=0;
        for(int end=0;end<arr.length;end++){
            currentsum+=arr[end];
            
            while(currentsum>target  &&  start<end){
                currentsum-=arr[start];
                start++;
            }
            
            if(currentsum==target){
                result.add(start+1);
                result.add(end+1);
                return result;
            }
        }
        
        result.add(-1);
        return result;
    }
}
