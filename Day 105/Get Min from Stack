class SpecialStack {
    Stack<Integer> st;
    int minval;
    
    public SpecialStack() {
        // Define Stack
        st=new Stack<>();
    }

    public void push(int x) {
        // Add an element to the top of Stack
        if(st.isEmpty()){
            st.push(x);
            minval=x;
        }else if(x>=minval){
            st.push(x);
        }else{
            st.push(2*x-minval);
            minval=x;
        }
    }

    public void pop() {
        // Remove the top element from the Stack
        if(st.isEmpty()) return;
        int top=st.pop();
        if(top<minval){
            minval=2*minval-top;
        }
    }

    public int peek() {
        // Returns top element of the Stack
        if(st.isEmpty()) return -1;
        int top=st.peek();
        if(top>=minval) return top;
        else return minval;
    }

    boolean isEmpty() {
        // Check if the stack is empty
        return st.isEmpty();
    }

    public int getMin() {
        // Finds minimum element of Stack
        if(st.isEmpty()) return -1;
        return minval;
    }
}
