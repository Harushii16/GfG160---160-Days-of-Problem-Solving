// User function Template for Java
class Solution {
    public static int[] productExceptSelf(int arr[]) {
        // code here
        int n=arr.length;
        int result[]=new int[n];
        int prefix=1;
        for(int i=0;i<n;i++){
            result[i]=prefix;
            prefix*=arr[i];
        }
        int suffix=1;
        for(int i=n-1;i>=0;i--){
            result[i]*=suffix;
            suffix*=arr[i];
        }
        
        return result;
    }
}
