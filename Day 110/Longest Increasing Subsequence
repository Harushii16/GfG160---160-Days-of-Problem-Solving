class Solution {
    static int lis(int arr[]) {
        // code here
        int n=arr.length;
        int tails[]=new int[n];
        int size=0;
        for(int x:arr){
int l=0,r=size-1;
while(l<=r){
    int mid=(l+r)/2;
    if(tails[mid]<x){
        l=mid+1;
        
    }else{
        r=mid-1;
    }
}

tails[l]=x;
if(l==size)size++;

        }
        
        return size;
    }
}
