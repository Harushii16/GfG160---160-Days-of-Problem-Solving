class Solution {

    static Boolean isSubsetSum(int arr[], int sum) {
        // code here
        int n=arr.length;
        boolean[] dp=new boolean[sum+1];
        dp[0]=true;
        for(int i=0;i<n;i++){
            for(int s=sum;s>=arr[i];s--){
                dp[s]=dp[s] || dp[s-arr[i]];
                
            }
        }
        return dp[sum];
    }
}
