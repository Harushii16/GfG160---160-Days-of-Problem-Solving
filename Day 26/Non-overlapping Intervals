class Solution {
    public int minRemoval(int intervals[][]) {
        // code here
        Arrays.sort(intervals,(a,b)->a[1]-b[1]);
        int n=intervals.length;
        int removals=0;
        int lastend=Integer.MIN_VALUE;
        
        for(int i=0;i<n;i++){
            if(intervals[i][0]>=lastend){
                lastend=intervals[i][1];
            }else{
                removals++;
            }
        }
        return removals;
    }
}
